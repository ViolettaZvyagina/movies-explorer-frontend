{
    "extends": "airbnb-base",
    "parser": "babel-eslint",
    "rules": {
      "no-underscore-dangle": ["error", {"allow": ["_id"]}],
      "no-console": "off",
      "linebreak-style": 0,
      "comma-dangle": ["error", "never"],
      "import/no-named-as-default": 0,
      "no-inner-declarations": 0,
      "consistent-return": 0
    }
  }
